<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <listener>
        <listener-class>org.jboss.test.capedwarf.testsuite.common.support.PullWorkerRegistration</listener-class>
    </listener>

    <servlet>
        <servlet-name>simple-counter</servlet-name>
        <servlet-class>org.jboss.test.capedwarf.testsuite.common.support.SimpleCounterServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>simple-counter</servlet-name>
        <url-pattern>/simplecounter</url-pattern>
    </servlet-mapping>
</web-app>
